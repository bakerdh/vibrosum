	language = {en},
	month = jul,
	note = {TLDR: Compared response properties of cells in the barreloids of the ventral posterior medial nucleus of the thalamus and the barrels in the rat primary somatosensory cortex were compared to assess their relative inhibitory and excitatory contributions to individual receptive fields.},
	number = {1},
	pages = {130--140},
	title = {Spatial gradients and inhibitory summation in the rat whisker barrel system},

	doi = {10.1167/jov.24.12.7},
	journal = {J Vis},
	journal-full = {Journal of vision},
	mesh = {Humans; Vision, Binocular; Contrast Sensitivity; Photic Stimulation; Color Perception; Perceptual Masking; Adult; Bayes Theorem; Sensory Thresholds; Male},
	month = {Nov},
	number = {12},
	pages = {7},
	pmc = {PMC11556357},
	pmid = {39499524},
	pst = {ppublish},
	title = {Binocular integration of chromatic and luminance signals},
	volume = {24},
	year = {2024},
	bdsk-url-1 = {https://doi.org/10.1167/jov.24.12.7}}


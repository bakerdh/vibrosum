	mesh = {Adolescent; Adult; Electrophysiology; Female; Fingers; Humans; Male; Mechanoreceptors; Physical Stimulation; Sensory Thresholds; Skin Temperature; Space Perception; Time Perception; Touch; Vibration},
	number = {2},

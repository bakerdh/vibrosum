	mesh = {Adult; Afferent Pathways; Brain Mapping; Dominance, Cerebral; Female; Functional Laterality; Hand; Humans; Magnetoencephalography; Male; Physical Stimulation; Somatosensory Cortex; Touch},
	month = {Sep},

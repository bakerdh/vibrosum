	issn = {0022-3751, 1469-7793},
	journal = {The Journal of Physiology},
	language = {en},
	month = nov,
	note = {TLDR: The effect of changing the orientation between the object and background is investigated and it is known that an object is less detectable when it is viewed against a background containing structures similar to the object.},
	number = {2},
	pages = {437--445},
	title = {Orientational selectivity of the human visual system},
	url = {https://physoc.onlinelibrary.wiley.com/doi/10.1113/jphysiol.1966.sp008101},

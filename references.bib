	journal = {Cerebral Cortex},
	language = {en},
	note = {TLDR: A novel purpose for cortical gain control is suggested: implementing optimal signal combination via mutual inhibition, perhaps explaining its ubiquity as a neural computation.},
	pages = {cercor;bhw395v1},
	title = {Evidence for an {Optimal} {Algorithm} {Underlying} {Signal} {Combination} in {Human} {Visual} {Cortex}},
